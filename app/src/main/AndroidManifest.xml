<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sira.collegephonebook">


<uses-permission android:name="android.permission.WRITE_SMS" />
    <uses-permission android:name="android.permission.SEND_SMS" />
     <uses-permission android:name="android.permision.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivityRegister"
            android:label = "Register Activity">

        </activity>
        <activity android:name=".ActivityLogin"
            android:label = "Login Activity">

        </activity>
        <activity android:name=".Authenticated"
            android:label = "Authentication Activity">
            </activity>

        <activity android:name=".ActivityContact"
            android:label = "ContactPage Activity">
        </activity>

        <activity android:name=".ActivityWebView"
            android:label = "WebView Activity">
        </activity>

    </application>

</manifest>
